<h1 class="page-title">Measure Written Year Range By Country</h1>

<table class="summary-table">
	
		<% Measure.measure_written_year_range_by_country.in_groups_of(10).each{|country_group| %>
			<tr>
				<% country_group.each{|country,first,last| %>
					<td>
						<span class="country-title"><%= country %></span>
						<p class="year-range"><%= first %> - <%= last %></p>
					</td>
				<% } %>
			</tr>
		<% } %>
	
</table>